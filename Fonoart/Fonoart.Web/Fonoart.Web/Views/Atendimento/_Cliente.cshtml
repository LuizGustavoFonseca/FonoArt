@model Fonoart.Web.Models.AtendimentoCadastroModel

<div class="col-md-12">
    <div class="panel panel-info">
        <div class="panel-heading">
            <h3 class="panel-title">Dados do Paciente</h3>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Código do Paciente*</label>
                        <input type="text" class="form-control" id="codigoPaciente" data-url-obter-cliente="@Url.Action("ObterPaciente", "Atendimento")" />
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-8">
                    <div class="form-group">
                        <label>Nome do Paciente*</label>
                        <input type="text" disabled="disabled" class="form-control" id="nomePaciente" />
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Telefone</label>
                        <input type="text" disabled="disabled" class="form-control" id="telefone" />
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-8">
                    <div class="form-group">
                        <label>Convênio*</label>
                        <select class="form-control" id="convenios" disabled="disabled">
                            <option value=""></option>
                            @foreach (var convenio in Model.Convenios)
                            {
                                <option value="@convenio.CodigoConvenio">@convenio.NomeConvenio</option>
                            }
                        </select>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Número da Carteirinha</label>
                        <input type="text" class="form-control" disabled="disabled" id="numeroCarteirinha" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
